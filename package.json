{
  "name": "Fonzi",
  "version": "1.0.0",
  "description": "Fonzi Toys",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch --source-map --no-stop-on-error --color scss/styles.scss:scss/_css/styles.css",
    "watch:postcss": "postcss scss/_css/styles.css -d css -u autoprefixer cssnano --source-map --watch",
    "start": "npm-run-all --parallel watch:*"
  },
  "keywords": [],
  "author": "Romero Arias Martin",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.0.17",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "sass": "^1.54.5"
  }
}